namespace CLHEP {

inline RandBreitWigner::RandBreitWigner(HepRandomEngine & anEngine,
                                        double a, double b )
: localEngine(&anEngine), deleteEngine(false), defaultA(a),
  defaultB(b) {}

inline RandBreitWigner::RandBreitWigner(HepRandomEngine * anEngine,
                                        double a, double b )
: localEngine(anEngine), deleteEngine(true), defaultA(a),
  defaultB(b) {}

}  // namespace CLHEP
