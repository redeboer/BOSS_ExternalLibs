inline
void G4CompositeCurve::Init(const G4CurveVector& segments0) 
{
  segments= segments0;
  lastIntersection.Reset();
  InitBounded();
}

inline
const G4CurveVector& G4CompositeCurve::GetSegments() const 
{
  return segments;
}
